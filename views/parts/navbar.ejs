                      <a <% if (page === "login"){ %>  class="nav-link active" <%  } else { %>  class="nav-link" <%}%> href="/login">Login</a>
                    </li>
                    <% } %> 

                    <% if(isLogged){%> 
                      <li class="nav-item">
                        <a  <% if (page === "Groups"){ %>  class="nav-link active" <%  } else { %>  class="nav-link" <%}%> href="/group">Groups</a>
                      </li>
                    <li class="nav-item">
                      <a  <% if (page === "Profile"){ %>  class="nav-link active" <%  } else { %>  class="nav-link" <%}%> href="/profile">Profile</a>
